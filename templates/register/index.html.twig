{% extends 'base.html.twig' %}

{% block title %}Hello RegisterController!
{% endblock %}

{% block content %}

	<h1 class="mt-3">Inscription</h1>

	{% for message in app.flashes() %}
		<div>
			{{ message }}
		</div>
	{% endfor %}

	{{form_start(form)}}
	{{form_row(form.firstName)}}
	{{form_row(form.lastName)}}
	{{form_row(form.email)}}
	{{form_row(form.password)}}
	{{form_row(form.confirmPassword)}}
	{{form_end(form)}}

{% endblock %}
